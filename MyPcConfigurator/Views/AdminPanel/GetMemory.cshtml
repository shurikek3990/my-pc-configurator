@model List<Memory>;
@{
    Layout = "_AdminPanelLayout";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Vendor</th>
            <th scope="col">Model</th>
            <th scope="col">Capacity</th>
            <th scope="col">RAM type</th>
            <th scope="col">Price</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <th scope="row">@m.Id</th>
                <td>@m.Vendor.Name</td>
                <td>@m.ModelName</td>
                <td>@m.Capacity</td>
                <td>@m.RamType</td>
                <td>@m.Price</td>
                <td>@Html.ActionLink("Edit", "AddOrUpdateMemory", "AdminPanel", new { @memoryId = m.Id })</td>
            </tr>
        }
    </tbody>
</table>

@Html.ActionLink("Add new memory", "AddOrUpdateMemory", "AdminPanel")
