@model List<Build>;

@{
    Layout = "_AdminPanelLayout";
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Created At</th>
            <th scope="col">Email</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var b in Model)
        {
            <tr>
                <th scope="row">@b.Id</th>
                <td>@b.CreatedAt</td>
                <td>@b.Email</td>
                <td>@Html.ActionLink("View", "ViewBuild", "AdminPanel", new { @buildId = b.Id })</td>
            </tr>
        }
    </tbody>
</table>
